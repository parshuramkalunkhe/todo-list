<%- include('header'); -%>
<div class="box" id="heading">
    <h1>
        <%= kindOfDay %> list.
    </h1>
</div>
<div class="box">
    <% for(var i=0; i<newListItem.length; i++ ) {%>
        <div class="item">
            <input type="checkbox" name="" id="">
            <p>
                <%= newListItem[i] %>
            </p>
        </div>
        <%}%>
            <form class="item" action="/" method="post">
                <input type="text" name="newItem" placeholder="New Stuff" autocomplete="off">
                <button type="submit" name="button">+</button>
            </form>
</div>
<%- include('footer'); -%>